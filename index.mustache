	<link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">

{{#prod}}
	<!-- ENV: PRODUCTION -->
	<link rel="stylesheet" type="text/css" href="{{cssFile}}"/>	
	<script src="{{jsFile}}" data-main="js/main" type="text/javascript"></script>	
{{/prod}}
{{^prod}}
